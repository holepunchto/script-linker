{
  "name": "script-linker",
  "version": "0.0.0",
  "description": "CJS/MJS source loader that can preresolve imports/requires so linking them on runtime runs much faster.",
  "main": "index.js",
  "author": "Holepunch",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && brittle test/*.test.*js",
    "lint": "standardx"
  },
  "dependencies": {
    "acorn": "^8.6.0",
    "b4a": "^1.3.1",
    "cjs-module-lexer": "^1.2.2",
    "detective": "^5.2.0",
    "es-module-lexer": "^0.9.3",
    "is-property": "^1.0.2",
    "resolve": "^1.20.0",
    "source-map": "^0.7.3",
    "unix-path-resolve": "^1.0.2",
    "xache": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "brittle": "^2.0.3",
    "jquery": "^3.6.0",
    "jsdom": "^19.0.0",
    "standardx": "^7.0.0",
    "thread-rpc": "^1.1.1"
  },
  "pre-commit": [
    "lint"
  ],
  "standardx": {
    "parser": "@babel/eslint-parser",
    "ignore": [
      "test/fixtures"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  }
}
